{
  "name": "legal-crm-frontend",
  "version": "1.0.0",
  "description": "Legal CRM System - Multi-Portal Frontend",
  "private": true,
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "dev:admin": "webpack serve --config webpack/webpack.dev.js --env portal=admin --port 3001",
    "dev:supervisor": "webpack serve --config webpack/webpack.dev.js --env portal=supervisor --port 3002",
    "dev:secretary": "webpack serve --config webpack/webpack.dev.js --env portal=secretary --port 3003",
    "dev:client": "webpack serve --config webpack/webpack.dev.js --env portal=client --port 3004",
    "dev:all": "concurrently \"npm run dev:admin\" \"npm run dev:supervisor\" \"npm run dev:secretary\" \"npm run dev:client\"",
    "build:admin": "webpack --config webpack/webpack.prod.js --env portal=admin",
    "build:supervisor": "webpack --config webpack/webpack.prod.js --env portal=supervisor",
    "build:secretary": "webpack --config webpack/webpack.prod.js --env portal=secretary",
    "build:client": "webpack --config webpack/webpack.prod.js --env portal=client",
    "build:all": "npm run clean && npm run build:admin && npm run build:supervisor && npm run build:secretary && npm run build:client",
    "clean": "rimraf dist",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,css,scss}\"",
    "type-check": "tsc --noEmit",
    "analyze": "webpack-bundle-analyzer dist/admin/stats.json",
    "prepare": "husky install"
  },
  "dependencies": {
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@hookform/resolvers": "^3.3.4",
    "@mui/icons-material": "^5.15.3",
    "@mui/lab": "^5.0.0-alpha.159",
    "@mui/material": "^5.15.3",
    "@mui/x-data-grid": "^6.18.7",
    "@mui/x-date-pickers": "^6.18.7",
    "@reduxjs/toolkit": "^2.0.1",
    "@stripe/react-stripe-js": "^2.4.0",
    "@stripe/stripe-js": "^2.2.2",
    "axios": "^1.6.5",
    "date-fns": "^3.2.0",
    "date-fns-tz": "^2.0.0",
    "i18next": "^23.7.16",
    "i18next-browser-languagedetector": "^7.2.0",
    "i18next-http-backend": "^2.4.2",
    "moment": "^2.30.1",
    "moment-timezone": "^0.5.44",
    "notistack": "^3.0.1",
    "pdf-lib": "^1.17.1",
    "react": "^18.2.0",
    "react-big-calendar": "^1.8.5",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-hook-form": "^7.48.2",
    "react-i18next": "^14.0.0",
    "react-intersection-observer": "^9.5.3",
    "react-pdf": "^7.6.0",
    "react-redux": "^9.1.0",
    "react-router-dom": "^6.21.1",
    "react-window": "^1.8.10",
    "recharts": "^2.10.4",
    "socket.io-client": "^4.7.4",
    "uuid": "^9.0.1",
    "yup": "^1.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.23.7",
    "@babel/preset-env": "^7.23.7",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@testing-library/jest-dom": "^6.2.0",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "@types/react": "^18.2.46",
    "@types/react-big-calendar": "^1.8.5",
    "@types/react-dom": "^18.2.18",
    "@types/react-pdf": "^7.0.0",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-window": "^1.8.8",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.18.0",
    "@typescript-eslint/parser": "^6.18.0",
    "babel-loader": "^9.1.3",
    "clean-webpack-plugin": "^4.0.0",
    "concurrently": "^8.2.2",
    "copy-webpack-plugin": "^12.0.1",
    "css-loader": "^6.8.1",
    "dotenv": "^16.3.1",
    "dotenv-webpack": "^8.0.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.6.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.0",
    "mini-css-extract-plugin": "^2.7.7",
    "prettier": "^3.1.1",
    "rimraf": "^5.0.5",
    "sass": "^1.69.7",
    "sass-loader": "^13.3.3",
    "style-loader": "^3.3.4",
    "terser-webpack-plugin": "^5.3.10",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "url-loader": "^4.1.1",
    "webpack": "^5.89.0",
    "webpack-bundle-analyzer": "^4.10.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "webpack-merge": "^5.10.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,scss,md}": [
      "prettier --write"
    ]
  }
}